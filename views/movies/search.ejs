<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
    <script
      src="https://code.jquery.com/jquery-3.2.1.min.js"
      integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
      crossorigin="anonymous">
    </script>
  </head>
  <body>

    <div class="container">
      <div id="searching">
      <h1 id="search-heading"> Search for a Movie</h1>
      <input id="movie-search" type="text" placeholder="find movie">
      <button id="search-button" class="btn btn-primary">Search</button>
      </div>
      <div id="results">
        <div id="poster"></div>
        <div id="caption"></div>
      </div>
    </div>
    <form id="submit-form" action="/movies" method="GET">
      <button id='save-button' class="btn btn-success">Save Post</button>
    </form>

    <script src="/js/app.js"></script>
  </body>
</html>
